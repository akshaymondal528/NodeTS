    "@types/node": "^20.8.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  }
